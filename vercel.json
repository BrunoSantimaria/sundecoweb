{
  "cleanUrls": true,
  "trailingSlash": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Robots-Tag",
          "value": "index,follow"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/contacto",
      "destination": "/contacto/",
      "permanent": true
    },
    {
      "source": "/redes",
      "destination": "/redes/",
      "permanent": true
    },
    {
      "source": "/cortinas",
      "destination": "/cortinas/",
      "permanent": true
    },
    {
      "source": "/cercos",
      "destination": "/cercos/",
      "permanent": true
    },
    {
      "source": "/toldos",
      "destination": "/toldos/",
      "permanent": true
    },
    {
      "source": "/valoranos",
      "destination": "/valoranos/",
      "permanent": true
    }
  ]
}
